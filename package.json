{
  "name": "UGOTLEAD-Express-API",
  "version": "0.0.1",
  "description": "This is an Express Server created for UGOTLEAD Product",
  "main": "functions/server.js",
  "scripts": {
    "prebuild": "cd functions/server && npm i",
    "build": "node ./create-env.js && netlify-lambda build express --config ./webpack.functions.js",
    "start": "nodemon server-local.js",
    "start:lambda": "netlify-lambda serve express --config ./webpack.functions.js"
  },
  "keywords": [
    "netlify",
    "express.js"
  ],
  "author": "AndreasLagoni",
  "license": "Apache-2.0",
  "devDependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^1.19.2",
    "serverless-http": "^2.3.0"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.47",
    "cors": "^2.8.5",
    "dotenv": "^8.5.1",
    "encoding": "^0.1.13",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "uuid": "^8.3.2"
  }
}
